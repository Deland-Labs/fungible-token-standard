{
  "canisters": {
    "graphql": {
      "type": "custom",
      "build": [
        "cargo build --target wasm32-unknown-unknown --package graphql --release",
        "ic-cdk-optimizer target/wasm32-unknown-unknown/release/graphql.wasm -o target/wasm32-unknown-unknown/release/graphql_opt.wasm"
      ],
      "candid": "src/graphql/src/graphql.did",
      "wasm": "target/wasm32-unknown-unknown/release/graphql_opt.wasm"
    },
    "dft": {
      "type": "custom",
      "build": [
        "cargo build --target wasm32-unknown-unknown --package  dft --release",
        "ic-cdk-optimizer target/wasm32-unknown-unknown/release/dft.wasm -o target/wasm32-unknown-unknown/release/dft_opt.wasm"
      ],
      "candid": "src/dft/src/token.did",
      "wasm": "target/wasm32-unknown-unknown/release/dft_opt.wasm"
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  },
  "version": 1
}